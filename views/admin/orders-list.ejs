        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List</h2>
                    <p>Lorem ipsum dolor sit amet.</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped">
                                    <thead class="thead-dark">
                                        <tr>
                                            <th>Sl No</th>
                                            <th>Order Id</th>
                                            <th>Customer Name</th>
                                            <th>Unit Price (₹)</th>
                                            <th>Status</th>
                                            <th>Date</th>
                                            <th class="text-center">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (locals.orders) { %>
                                            <% for (let i = 0; i < orders.length; i++) { %>
                                                <tr>
                                                    <td><%= i + 1 %></td>
                                                    <td><%= orders[i]._id %></td>
                                                    <td><%= orders[i].address[0].name %></td>
                                                    <td><%= orders[i].items.price %></td>
                                                    <td><span class="badge rounded-pill bg-success"><%= orders[i].orderStatus %></span></td>
                                                    <td><%= orders[i].createdAt.toLocaleString() %></td>
                                                    <td class="text-center">
                                                        <a href="/admin/order-detail/<%= orders[i]._id %>" class="btn btn-sm btn-info">Detail</a>
                                                        <!-- <div class="dropdown d-inline">
                                                            <button class="btn btn-sm btn-light dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                                                                More
                                                            </button>
                                                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                                <li><a class="dropdown-item" href="#">View Detail</a></li>
                                                                <li><a class="dropdown-item" href="#">Edit Info</a></li>
                                                                <li><a class="dropdown-item text-danger" href="#">Delete</a></li>
                                                            </ul>
                                                        </div> -->
                                                    </td>
                                                </tr>
                                            <% } %>
                                        <% } %>
                                    </tbody>
                                </table>
                            </div>
                            <!-- table-responsive //end -->
        
                            <% if (totalPages > 1) { %>
                                <div class="pagination-area mt-4">
                                    <nav aria-label="Page navigation">
                                        <ul class="pagination justify-content-center">
                                            <% if (page > 1) { %>
                                                <li class="page-item">
                                                    <a class="page-link" href="/admin/order-list?page=<%= page - 1 %>" aria-label="Previous">
                                                        <span aria-hidden="true">&laquo;</span>
                                                    </a>
                                                </li>
                                            <% } %>
                                            <% for (let i = 1; i <= totalPages; i++) { %>
                                                <li class="page-item <%= i === page ? 'active' : '' %>">
                                                    <a class="page-link" href="/admin/order-list?page=<%= i %>"><%= i %></a>
                                                </li>
                                            <% } %>
                                            <% if (page < totalPages) { %>
                                                <li class="page-item">
                                                    <a class="page-link" href="/admin/order-list?page=<%= page + 1 %>" aria-label="Next">
                                                        <span aria-hidden="true">&raquo;</span>
                                                    </a>
                                                </li>
                                            <% } %>
                                        </ul>
                                    </nav>
                                </div>
                            <% } %>
                        </div>
                        <!-- card-body end// -->
