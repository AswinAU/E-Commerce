        <main class="main-wrap">
            <section class="content-main">
                <div class="content-header">
                    <div>
                        <h2 class="content-title card-title">Categories </h2>
                        <p>Add, edit or delete a category</p>
                    </div>
                    <div>
                        <!-- <input type="text" placeholder="Search Categories" class="form-control bg-white"> -->
                        <form action="/admin/searchCat" method="post">
                            <div class="cupon_text d-flex"  >
                                <input type="text" class="ms-auto " style="width: 20rem;"name="search"  placeholder="search" id="coupon">
                                <button class="btn btn-brand " type="submit">search</button>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-3">
                                <form action="/admin/add-category" method="post" enctype="multipart/form-data">
                                    <div class="mb-4">
                                        <label for="product_name" class="form-label">Name</label>
                                        <input name="category" type="text" placeholder="Type here" class="form-control"
                                            id="product_name" required="true" />
                                            
                                            <% if(locals.error){%>
                                            <p  class="text-danger" >
                                                <%if(error){%>
                                               category already exists    <%}%>
                                             </p>
                                             <%}%>
                                              
                                    </div>
                                    <div class="mb-4">
                                        <label for="product_slug" class="form-label"> Price</label>
                                        <input name="basePrice" type="number" min="0" placeholder="Type here"
                                            class="form-control" id="base_price" required="true" />
                                    </div>
                                    <div class="mb-4">
                                        <label for="image" class="form-label">Cover Image</label>
                                        <input name="image" type="file" placeholder="Type here" class="form-control"
                                            id="image" required="true" />
                                    </div>

                                    <div class="mb-4">
                                        <label class="form-label">Description</label>
                                        <textarea name="description" placeholder="Type here" class="form-control"
                                            required="true"></textarea>
                                    </div>
                                    <div class="d-grid">
                                        <button class="btn btn-primary"    type="submit" >Create category</button>
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-9">
                                <div class="table-responsive">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th>No</th>
                                                <th>Image</th>
                                                <th>Category</th>
                                                <th>Description</th>
                                                <th>Base Price</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (i=0; i < data.length; i++) { %>
                                                <tr>
                                                    <td>
                                                        <%= i+1 %>
                                                    </td>
                                                    <td>
                                                        <img src="upload/category/<%= data[i].image%>" width="80px"
                                                            alt="img">
                                                    </td>
                                                    <td>
                                                        <%= data[i].category %>
                                                    </td>
                                                    <td>
                                                        <%= data[i].description%>
                                                    </td>
                                                    <td>
                                                        <%= data[i].base_price%>
                                                    </td>
                                                    <td>
                                                        <a href="/admin/edit-C/<%=data[i]._id%>" class="btn btn-info p-1">Edit</a>
                                                        <a href="/admin/delete-category/<%=data[i]._id%>"
                                                            class="btn btn-danger p-1">Delete</a>
                                                    </td>
                                                </tr>
                                                <% } %>

                                        </tbody>
                                    </table>
                                </div>
                            </div> <!-- .col// -->








                            
                        </div> <!-- .row // -->
                    </div> <!-- card body .// -->
                </div> <!-- card .// -->
            </section> <!-- content-main end// -->
            <footer class="main-footer font-xs">
                <div class="row pb-30 pt-15">
                    <div class="col-sm-6">
                    
                
                           


                            <% if (locals. totalPages) { %>
                                <div class="pagination">
                                  <% if (currentPage > 1) { %>
                                    <button><a href="?page=<%= currentPage - 1 %>" style="color:black; text-decoration: none;" >Previous</a></button>
                                  <% } %>
                              
                                  <% for (let i = 1; i <= totalPages; i++) { %>
                                    <button><a href="?page=<%= i %>"  style="color:black; text-decoration: none;" class="<%= i === currentPage ? 'active' : '' %>"><%= i %></a></button>
                                  <% } %>
                              
                                  <% if (currentPage < totalPages) { %>
                                    <button><a href="?page=<%= currentPage + 1 %>" style="color:black; text-decoration: none;" >Next</a></button>
                                  <% } %>
                                </div>
                                <%}%>

                                    </ul>
                                </nav>
